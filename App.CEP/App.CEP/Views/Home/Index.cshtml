@model Localizacao

@{
    ViewData["Title"] = "Home Page";
}

<h5>Consulta de CEP</h5>
<hr />
<form asp-action="Index" asp-controller="Home">

    <div class="form-group">
        <input asp-for="Cep" placeholder="informe o cep" class="form-control"/>
        <input type="submit" value="Pesquisar" class="btn btn-primary"/>
    </div>

    <div class="form-group">
        <label for="Logradouro">Logradouro</label>
        <input asp-for="Logradouro" class="form-control"/>
    </div>

    <div class="form-group">
        <label for="Complemento">Complemento</label>
        <input asp-for="Complemento" class="form-control"/>
    </div>

    <div class="form-group">
        <label for="Bairro">Bairro</label>
        <input asp-for="Bairro" class="form-control"/>
    </div>

    <div class="form-group">
        <label for="Localidade">Localidade</label>
        <input asp-for="Localidade" class="form-control"/>
    </div>

    <div class="form-group">
        <label for="UF">UF</label>
        <input asp-for="UF" class="form-control"/>
    </div>

    <div class="form-group">
        <label for="Ibge">IBGE</label>
        <input asp-for="Ibge" class="form-control"/>
    </div>
    <div class="form-group">
        <label for="Gia">GIA</label>
        <input asp-for="Gia" class="form-control"/>
    </div>
    <div class="form-group">
        <label for="DDD">DDD</label>
        <input asp-for="DDD" class="form-control"/>
    </div>
    <div class="form-group">
        <label for="Siafi">SIAFI</label>
        <input asp-for="Siafi" class="form-control"/>
    </div>
</form>
